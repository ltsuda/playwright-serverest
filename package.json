{
    "name": "playwright-serverest",
    "description": "Estudo de Teste de API utilizando Playwright/Test e [ServeRest](https://serverest.dev/)",
    "version": "1.0.0",
    "devDependencies": {
        "@playwright/test": "^1.16.2",
        "faker": "^5.5.3",
        "joi": "^17.4.2",
        "serverest": "^2.24.4"
    },
    "scripts": {
        "test": "PWTEST_SKIP_TEST_OUTPUT=1 npx playwright test ||:",
        "test:match": "PWTEST_SKIP_TEST_OUTPUT=1 npx playwright test --grep $MATCH",
        "reporter:html": "npx playwright show-report test-results"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ltsuda/playwright-serverest.git"
    },
    "keywords": [
        "playwright",
        "javascript",
        "serverest",
        "automation",
        "api",
        "e2e"
    ],
    "author": "Leonardo Tsuda",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ltsuda/playwright-serverest/issues"
    },
    "homepage": "https://github.com/ltsuda/playwright-serverest#readme"
}
